    "def set_visible_GPUs(accessible=[0]):\n",
    "    os.environ[\"CUDA_DEVICE_ORDER\"] = \"PCI_BUS_ID\"   # See issue #152 stack-overflow.\n",
    "    accessible = '\"' + ','.join(str(e) for e in accessible) + '\"'\n",
    "    os.environ[\"CUDA_VISIBLE_DEVICES\"] = accessible\n",
    "\n",
    "\n",
    "set_visible_GPUs([1])"
   ]
